{
  "name": "crewcodegen-live",
  "version": "2.0.0",
  "description": "Real-time AI code generation with multi-agent workflows",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx watch server/src/index.ts",
    "dev:client": "cd client && npm run dev",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc --project server.tsconfig.json",
    "build:client": "cd client && npm run build",
    "start": "node dist/server/index.js",
    "preview": "npm run build && npm start",
    "check": "tsc --noEmit",
    "clean": "rimraf dist",
    "install:all": "npm install && cd client && npm install",
    "test": "echo \"Tests coming soon\" && exit 0"
  },
  "dependencies": {
    "@cerebras/cerebras_cloud_sdk": "^1.50.0",
    "express": "^4.21.2",
    "socket.io": "^4.8.1",
    "cors": "^2.8.5",
    "helmet": "^8.1.0",
    "compression": "^1.8.1",
    "nanoid": "^5.1.5",
    "zod": "^3.24.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/node": "^20.16.11",
    "typescript": "^5.6.3",
    "tsx": "^4.19.1",
    "rimraf": "^6.0.1",
    "concurrently": "^9.1.0"
  },
  "keywords": ["ai", "code-generation", "multi-agent", "real-time", "cerebras", "crewai"],
  "author": "CrewCodeGen Team",
  "license": "MIT"
}

